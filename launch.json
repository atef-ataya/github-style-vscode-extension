{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run CodeStyle Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/vscode-extension"
      ],
      "outFiles": ["${workspaceFolder}/vscode-extension/out/**/*.js"]
    }
  ]
}
