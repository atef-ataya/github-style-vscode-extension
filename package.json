{
  "name": "codestyle-stylegen",
  "displayName": "CodeStyle Code Generator",
  "description": "Analyze GitHub repos and generate consistent code using your own style.",
  "version": "0.0.1",
  "publisher": "your-publisher-id",
  "repository": {
    "type": "git",
    "url": "https://github.com/atef-ataya/github-style-vscode-extension"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codestyle.generate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codestyle.generate",
        "title": "CodeStyle Code Generator"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext .ts,.js",
    "tsc": "tsc -p ./",
    "test": "node src/tests/basic.test.js",
    "package": "vsce package"
  },
  "dependencies": {
    "@octokit/rest": "^20.1.1",
    "dotenv": "^16.4.5",
    "openai": "^4.20.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "typescript": "^5.4.0",
    "vsce": "^2.15.0"
  }
}
