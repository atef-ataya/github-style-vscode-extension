{
  "name": "trae-ai-stylegen",
  "displayName": "GitStyle AI Code Generator",
  "description": "Analyze GitHub repos and generate consistent code using your own style.",
  "version": "0.0.1",
  "publisher": "your-publisher-id",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:trae-ai.generate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "trae-ai.generate",
        "title": "GitStyle AI Code Generator"
      }
    ]
  },
  "activationEvents": [
    "onCommand:trae-ai.generate"
  ],
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "typescript": "^5.4.0",
    "vsce": "^2.15.0"
  }
}
